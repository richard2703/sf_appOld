{
  "name": "cordovaframework7vuewebpacktemplate",
  "version": "1.0.0",
  "description": "Brand new cordova project with framework7 - vue - webpack!",
  "scripts": {
    "default": "echo \"There is no scripts you can run with npm run command.\" && exit 1",
    "build": "npm run clean && webpack",
    "clean": "rimraf www",
    "dev:browser": "cordova run browser -- --lr",
    "dev:android": "cordova run android -- --lr --device",
    "dev:android-emulate": "cordova run android -- --lr --emulate",
    "dev:ios": "cordova run ios -- --lr",
    "init": "npm install && npm run build && npm run add:platforms && npm run dev:browser"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "license": "ISC",
